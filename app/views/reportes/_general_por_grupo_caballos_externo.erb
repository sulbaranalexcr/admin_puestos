<script>
  function imprimir(tipo){
    desde = $('#desde').val();
    hasta = $('#hasta').val();
    url = '/reportes/cuadre_general_por_agentes_externo';
    window.open ( url + ".pdf?desde=" +desde+"&hasta="+hasta, '_target' );
  }

  function exportar(tipo){
    desde = $('#desde').val();
    hasta = $('#hasta').val();
      url = '/reportes/cuadre_general_por_agentes_externo';
    window.open ( url + ".xlsx?desde=" +desde+"&hasta="+hasta, '_target' );
  }


</script>
<div class="box-footer">
  <input type="button" value="Imprimir" class="btn btn-success" onclick="imprimir('<%=@tipo_reporte_url%>')"/>
  <input type="button" value="Exportar Excel" class="btn btn-success" onclick="exportar('<%=@tipo_reporte_url_excel%>')"/>
</div>
<div class="row">
