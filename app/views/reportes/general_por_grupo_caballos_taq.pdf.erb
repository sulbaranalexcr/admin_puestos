                            <th  style="text-align: right;">Perdio O/A</th>
                            <th  style="text-align: right;">Comisi&oacute;n O/A</th>
                            <th  style="text-align: right;">Comisi&oacute;n Banca</th>
                          </tr>
                        </thead>
                        <tbody>
                          <%total1 = 0%>
                          <%total2 = 0%>
                          <%total3 = 0%>
                          <%total4 = 0%>
                          <%total5 = 0%>
                          <%total6 = 0%>
                          <%total7 = 0%>
                          <%total8 = 0%>
                          <%total9 = 0%>
                          <%total10 = 0%>
                          <% @reporte.each do |repo| %>
                            <tr>
                              <td> <%= repo['nombre'] %> </td>
                              <td style="text-align: right;"> <%= number_with_precision(repo['gano_oc'], precision: 2, separator: '.', delimiter: ',') %> </td>
                              <td style="text-align: right;"> <%= number_with_precision(repo['perdio_oc'], precision: 2, separator: '.', delimiter: ',') %> </td>
                              <td style="text-align: right;"> <%= number_with_precision( repo['comision_oc'].to_f , precision: 2, separator: '.', delimiter: ',') %> </td>
                              <td style="text-align: right;"> <%= number_with_precision( ( repo['comision_oc'].to_f * repo['comision_banca'].to_f) / 100 , precision: 2, separator: '.', delimiter: ',') %>  </td>
                            </tr>
                            <%total2 += repo['gano_oc'].to_f%>
                            <%total3 += repo['premio'].to_f + repo['gano_oc'].to_f%>
                            <%total4 += repo['comision_oc'].to_f%>
                            <%total5 += (repo['comision_oc'].to_f * repo['comision_banca'].to_f) / 100%>
                          <% end %>
                        </tbody>
                        <tfoot style="background-color: #A4A4A4;">
                          <tr>
                            <td style="font-weight: bold;"> Totales </td>
                            <td style="text-align: right;font-weight: bold;"> <%= number_with_precision( total2 , precision: 2, separator: '.', delimiter: ',')%> </td>
                            <td style="text-align: right;font-weight: bold;"> <%= number_with_precision( total3 , precision: 2, separator: '.', delimiter: ',')%> </td>
                            <td style="text-align: right;font-weight: bold;"> <%= number_with_precision( total4 , precision: 2, separator: '.', delimiter: ',')%> </td>
                            <td style="text-align: right;font-weight: bold;"> <%= number_with_precision( total5 , precision: 2, separator: '.', delimiter: ',')%> </td>
                          </tr>
                        </tfoot>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


