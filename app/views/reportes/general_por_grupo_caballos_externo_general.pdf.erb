
<html>
<head>
  <title></title>
  	<%=wicked_pdf_stylesheet_link_tag  'bootstrap.css'%>
</head>

<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Reporte Cuadre General Taquillas</h3>
      </div>
      <div class="box-body">
        <div id="user_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
          <H4>Desde: <%=@desde%> / Hasta: <%=@hasta%></H4>
          <H4>General</H4>
          <div class="row">
            <div class="col-sm-12" style="font-size: 12px;">
             <table id="user_index" class="table table-bordered table-striped dataTable" role="grid" style="width: 100%;">
                <thead>
                  <tr>
                    <th style="font-size: 12px;text-align: left;width: 10%">Nombre</th>
                    <th style="font-size: 12px;text-align: right;width: 15%">Ventas</th>
                    <th style="font-size: 12px;text-align: right;width: 15%">Premios</th>
                    <th style="font-size: 12px;text-align: right;width: 15%">Balance</th>
                    <th style="font-size: 12px;text-align: right;width: 15%">Mi Comisi&oacute;n <br>2.5%</th>
                    <th style="font-size: 12px;text-align: right;width: 15%">Mi Comisi&oacute;n <br>5%</th>
                    <th style="font-size: 12px;text-align: right;width: 15%">Total<br>Comisiones</th>
                  </tr>
                </thead>
                <tbody>
                  <%total1 = 0%>
                  <%total2 = 0%>
                  <%total3 = 0%>
                  <%total4 = 0%>
                  <%total5 = 0%>
                  <%total6 = 0%>
                  <%total7 = 0%>
                  <%total8 = 0%>
                  <%total9 = 0%>
                  <%total10 = 0%>
                  <% @reporte.each do |repo| %>
                  <%recibir = repo['monto_otro_grupo'].to_f > 0 ? repo['monto_otro_grupo'].to_f : 0 %>
                  <%pagar = repo['monto_otro_grupo'].to_f < 0 ? (repo['monto_otro_grupo'] * -1).to_f : 0 %>
                    <tr>
                      <td style="font-size: 12px;"> <%= repo['nombre'] %> </td>
                      <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( repo['venta'] , precision: 2, separator: '.', delimiter: ',')%> </td>
                      <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( repo['premio'].to_f, precision: 2, separator: '.', delimiter: ',') %> </td>
                      <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( repo['venta'].to_f - repo['premio'] , precision: 2, separator: '.', delimiter: ',') %> </td>
                      <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( repo['comision_oc'].to_f  , precision: 2, separator: '.', delimiter: ',') %> </td>
                      <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( repo['comision'].to_f, precision: 2, separator: '.', delimiter: ',') %> </td>
                      <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( repo['comision_oc'].to_f  + repo['comision'].to_f , precision: 2, separator: '.', delimiter: ',') %>  </td>
                    </tr>
                    <%total1 += repo['venta'].to_f%>
                    <%total2 += repo['premio'].to_f %>
                    <%total3 += repo['venta'].to_f - repo['premio'] %>
                    <%total4 += repo['comision_oc'].to_f %>
                    <%total5 += repo['comision'].to_f %>
                    <%total6 += repo['comision_oc'].to_f  + repo['comision'].to_f %>


                  <% end %>
                </tbody>
                <tfoot style="background-color: #A4A4A4;">
                  <tr>
                    <td style="font-size: 12px;"> Totales </td>
                    <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( total1 , precision: 2, separator: '.', delimiter: ',')%> </td>
                    <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( total2 , precision: 2, separator: '.', delimiter: ',')%> </td>
                    <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( total3 , precision: 2, separator: '.', delimiter: ',')%> </td>
                    <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( total4 , precision: 2, separator: '.', delimiter: ',')%> </td>
                    <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( total5 , precision: 2, separator: '.', delimiter: ',')%> </td>
                    <td style="font-size: 12px;text-align: right;"> <%= number_with_precision( total6 , precision: 2, separator: '.', delimiter: ',')%> </td>
                  </tr>
                </tfoot>
              </table>

            </div>
          </div>
          




        </div>
      </div>
    </div>


