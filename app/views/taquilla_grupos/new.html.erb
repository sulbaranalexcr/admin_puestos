<%= form_for @taquilla, url: @url, role: :form, html: { multipart: fal } do |f| %>
  <div class="row">
    <div class="col-xs-12">
      <div class="box">

        <div class="box-header">
          <h3 class="box-title">Nueva Taquilla</h3>
        </div>

        <div class="box-body">
          <%= render partial: 'taquilla_grupos/form', locals: { f: f } %>
        </div>

        <div class="box-footer">
          <%= link_to 'Cancelar', taquilla_grupos_path, class: 'btn btn-danger' %>
          <%= f.submit 'Enviar', class: "btn btn-success" %>
        </div>

      </div>
    </div>
  </div>
<% end %>
