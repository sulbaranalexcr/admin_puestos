<%= form_for @match, url: @url, role: :form, html: { multipart: true } do |f| %>
<div class="row">
  <div class="col-xs-12">
    <div class="box">

      <div class="box-header">
        <h3 class="box-title">Editando Match</h3>
      </div>

      <div class="box-body">
        <%= render partial: 'matchs/form_edit', locals: { f: f } %>
      </div>

      <div class="box-footer">
        <%= link_to 'Cancelar', matchs_path, class: 'btn btn-danger' %>
        <%= f.submit 'Enviar', class: "btn btn-success"%>
      </div>

    </div>
  </div>
</div>

<% end %>
