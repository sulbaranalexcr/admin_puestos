<%= form_for @match, url: @url, role: :form, html: { multipart: true } do |f| %>
<div class="row">
  <div class="col-xs-12">
    <div class="box">

      <div class="box-header">
        <h3 class="box-title">Nuevo Match</h3>
      </div>

      <div class="box-body">
        <%= render partial: 'matchs/form', locals: { f: f } %>
      </div>

      <div class="box-footer">
        <%= link_to 'Cancelar', matchs_path, class: 'btn btn-danger' %>
        <input type="submit" name="commit" value="Enviar" class="btn btn-success" data-disable-with="Enviar"  id="boton_enviar_match">
      </div>

    </div>
  </div>
</div>

<% end %>
